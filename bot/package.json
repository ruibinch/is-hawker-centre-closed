{
  "name": "is-hawker-centre-closed-bot",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "dev": "sls offline start",
    "init:db": "yarn run:db:dev:create && yarn run:db:prod:create && yarn run:db:dev:seed && yarn run:db:prod:seed",
    "init:bot": "yarn init:bot:prod",
    "init:bot:dev": "ts-node src/scripts/initBot.ts",
    "init:bot:prod": "NODE_ENV=production yarn init:bot:dev",
    "deploy:all": "yarn deploy:dev && yarn deploy:prod",
    "deploy:all:bot": "yarn deploy:dev:bot && yarn deploy:prod:bot",
    "deploy:dev": "sls deploy -s dev",
    "deploy:dev:bot": "sls deploy -s dev -f bot",
    "deploy:prod": "sls deploy -s prod",
    "deploy:prod:bot": "sls deploy -s prod -f bot",
    "remove:all": "yarn remove:dev && yarn remove:prod",
    "remove:dev": "sls remove -s dev",
    "remove:prod": "sls remove -s prod",
    "run:db:dev:create": "ts-node src/scripts/manageDb.ts create",
    "run:db:dev:delete": "ts-node src/scripts/manageDb.ts delete",
    "run:db:dev:reset": "ts-node src/scripts/resetAndUpdateDb.ts",
    "run:db:dev:seed": "ts-node src/scripts/seedDb.ts",
    "run:db:prod:create": "NODE_ENV=production yarn run:db:dev:create",
    "run:db:prod:delete": "NODE_ENV=production yarn run:db:dev:delete",
    "run:db:prod:reset": "NODE_ENV=production yarn run:db:dev:reset",
    "run:db:prod:seed": "NODE_ENV=production yarn run:db:dev:seed",
    "run:backup:dev": "ts-node src/scripts/runBackup.ts",
    "run:backup:prod": "NODE_ENV=production yarn run:backup:dev",
    "run:restore:dev": "ts-node src/scripts/runRestore.ts",
    "run:restore:prod": "NODE_ENV=production yarn run:restore:dev",
    "run:add-closure:dev": "ts-node src/scripts/manageClosureTable.ts add",
    "run:add-closure:prod": "NODE_ENV=production yarn run:add-closure:dev",
    "run:delete-closure:dev": "ts-node src/scripts/manageClosureTable.ts delete",
    "run:delete-closure:prod": "NODE_ENV=production yarn run:delete-closure:dev",
    "run:scan-new": "NODE_ENV=production ts-node src/scripts/scanNewEntries.ts",
    "run:print-commands": "ts-node src/scripts/printCommands.ts",
    "run:test-api": "ts-node src/scripts/testApi.ts"
  },
  "devDependencies": {
    "@types/i18n-js": "^3.8.0",
    "i18n-extract": "^0.6.7"
  },
  "dependencies": {
    "@sentry/serverless": "^6.5.1",
    "discord.js": "^12.5.3",
    "emoji-regex": "^9.2.2",
    "i18n-js": "^3.8.0",
    "jshashes": "^1.0.8",
    "node-cache": "^5.1.2"
  }
}
