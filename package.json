{
  "name": "is-hawker-centre-closed",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "dev": "sls offline start",
    "init:db": "yarn db:create && yarn db:seed:dev && yarn db:seed:prod",
    "init:bot": "yarn init:bot:prod",
    "init:bot:dev": "ts-node src/scripts/initBot.ts",
    "init:bot:prod": "NODE_ENV=production yarn init:bot:dev",
    "db:create": "ts-node src/scripts/initDb.ts create",
    "db:delete": "ts-node src/scripts/initDb.ts delete",
    "db:seed:dev": "ts-node src/dataCollection/index.ts",
    "db:seed:prod": "NODE_ENV=production yarn db:seed:dev",
    "deploy:both:all": "yarn deploy:dev:all && yarn deploy:prod:all",
    "deploy:both:bot": "yarn deploy:dev:bot && yarn deploy:prod:bot",
    "deploy:dev:all": "sls deploy -s dev",
    "deploy:prod:all": "sls deploy -s prod",
    "deploy:dev:bot": "sls deploy -s dev -f bot",
    "deploy:prod:bot": "sls deploy -s prod -f bot",
    "remove:all": "yarn remove:dev && yarn remove:prod",
    "remove:dev": "sls remove -s dev -f bot",
    "remove:prod": "sls remove -s prod -f bot",
    "print:bot-commands": "ts-node src/bot/commands.ts",
    "run:backup": "ts-node src/scripts/runBackup.ts",
    "lint": "eslint . --ext .ts",
    "test": "TZ=UTC jest"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.75",
    "@types/i18n-js": "^3.8.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.37",
    "@types/pdf-parse": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.5.3",
    "prettier": "^2.2.1",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^6.9.0",
    "serverless-offline-scheduler": "^0.4.2",
    "serverless-webpack": "^5.4.1",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3",
    "webpack": "^5.31.2",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "aws-sdk": "^2.884.0",
    "axios": "^0.21.1",
    "date-fns": "^2.20.1",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.2.2",
    "i18n-js": "^3.8.0",
    "jshashes": "^1.0.8",
    "pdf-parse": "^1.1.1"
  }
}
