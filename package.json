{
  "name": "is-hawker-centre-closed",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "dev": "sls offline start",
    "init:db": "yarn run:db:dev:create && yarn run:db:prod:create && yarn run:db:dev:seed && yarn run:db:prod:seed",
    "init:bot": "yarn init:bot:prod",
    "init:bot:dev": "ts-node src/scripts/initBot.ts",
    "init:bot:prod": "NODE_ENV=production yarn init:bot:dev",
    "deploy:all": "yarn deploy:dev && yarn deploy:prod",
    "deploy:all:bot": "yarn deploy:dev:bot && yarn deploy:prod:bot",
    "deploy:dev": "sls deploy -s dev",
    "deploy:dev:bot": "sls deploy function -s dev -f bot",
    "deploy:prod": "sls deploy -s prod",
    "deploy:prod:bot": "sls deploy function -s prod -f bot",
    "remove:all": "yarn remove:dev && yarn remove:prod",
    "remove:dev": "sls remove -s dev",
    "remove:prod": "sls remove -s prod",
    "run:db:dev:create": "CLI_OPERATION=create ts-node src/scripts/manageDb.ts",
    "run:db:dev:delete": "CLI_OPERATION=delete ts-node src/scripts/manageDb.ts",
    "run:db:dev:sync": "ts-node src/scripts/syncDb.ts",
    "run:db:dev:seed": "ts-node src/scripts/seedDb.ts",
    "run:db:prod:create": "NODE_ENV=production yarn run:db:dev:create",
    "run:db:prod:delete": "NODE_ENV=production yarn run:db:dev:delete",
    "run:db:prod:sync": "NODE_ENV=production yarn run:db:dev:sync",
    "run:db:prod:seed": "NODE_ENV=production yarn run:db:dev:seed",
    "run:backup:dev": "ts-node src/scripts/runBackup.ts",
    "run:backup:prod": "NODE_ENV=production yarn run:backup:dev",
    "run:restore:dev": "ts-node src/scripts/runRestore.ts",
    "run:restore:prod": "NODE_ENV=production yarn run:restore:dev",
    "run:health:dev": "ts-node src/scripts/checkHealthiness.ts",
    "run:health:prod": "NODE_ENV=production yarn run:health:dev",
    "run:scan-new": "NODE_ENV=production ts-node src/scripts/scanNewEntries.ts",
    "run:print-commands": "ts-node src/scripts/printBotCommands.ts",
    "run:print-description": "ts-node src/scripts/printBotDescription.ts",
    "deduplicate": "npx yarn-deduplicate yarn.lock && yarn install",
    "lint": "eslint . --ext .ts",
    "test": "TZ=UTC jest",
    "prettify": "prettier --write \"**/*.{ts,js,json}\""
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.89",
    "@types/i18n-js": "3.8.2",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.5",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "aws-sdk": "2.1048.0",
    "eslint": "8.5.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.3.2",
    "eslint-plugin-prettier": "4.0.0",
    "i18n-extract": "0.6.7",
    "jest": "27.4.5",
    "prettier": "2.5.1",
    "serverless-offline": "8.3.1",
    "serverless-webpack": "5.6.0",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "webpack": "5.65.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@sentry/serverless": "6.16.1",
    "axios": "0.24.0",
    "date-fns": "2.28.0",
    "discord.js": "^12.5.3",
    "dotenv": "10.0.0",
    "emoji-regex": "10.0.0",
    "i18n-js": "3.8.0",
    "jshashes": "1.0.8",
    "node-cache": "5.1.2"
  }
}
